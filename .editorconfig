# EditorConfig file for a Go project
# See https://editorconfig.org for more details
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.go]
indent_style = tab
indent_size = 8
max_line_length = 100

[*.{yml,yaml,json}]
indent_style = space
indent_size = 2
